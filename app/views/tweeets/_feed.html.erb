<div class="col-md-6">
  <% if user_signed_in? %>
  <% render 'form' %>
  <% end %>
  <%= render 'tweeets/form' %>
  <% @tweeets.each do | tweeet | %>
    <div class="box">
      <article class="media">
        <div class="media-left">
          <figure class="image is-64x64">
          </figure>
        </div>
        <div class="media-content">
          <div class="content">
            <p><%= tweeet.tweeet %></p>
          </div>
        </article>
      </div>
    <% end %>
  </div>
